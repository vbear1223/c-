//Basic and Printing Using Variables (9 points)

#include <iostream>
using namespace std;
int main() {
    string subject;
    int room;
    double e; 
    subject = "Computer Science";
    room = 113;
    e = 2.71828;
    
    cout << "This is room # "<< room <<"\n";
    cout << "e is close to "<< e<<"\n";
    cout << "I am learning a bit about "<< subject<<"\n";
	}
